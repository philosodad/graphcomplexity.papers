	Keywords = {access protocols, ad hoc networks, queueing theory, telecommunication signalling, ultra wideband communication, wireless channelsDYNAMMA, MAC framework, dynamic multichannel medium access framework, multichannel medium access control, multiple channels, queueing delay reduction, spatial re-use, system capacity, ultra-wideband UWB radio, wireless ad hoc networks},
	Month = {Oct.},
	Pages = {1-11},
	Title = {DYNAMMA: A DYNAmic Multi-channel Medium Access Framework for Wireless Ad Hoc Networks},
	Year = {2007},
	Bdsk-File-1 = {},
