	Journal = {Journal of The Royal Society Interface},
	Keywords = {biological modelling, reactive animation, emergence properties, reactive system, biological modeling, system modeling, emergence},
	Number = {13},
	Pages = {175-182},
	Title = {{Explaining a complex living system: dynamics, multi-scaling and emergence}},
