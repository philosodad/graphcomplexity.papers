	Keywords = {immune system, immune system modeling, simulation,},
	Number = {11},
	Pages = {2485-2497},
	Title = {{Toward Rigorous Comprehension of Biological Complexity: Modeling, Execution, and Visualization of Thymic T-Cell Maturation}},
