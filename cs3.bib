	Group = {immune; immsim},
	Keywords = {simulation},
	Local-Url = {file://localhost/Users/paul/Documents/Biblio/01428691.pdf},
	Title = {Computational modeling of the immune response to tumor antigens: implications for vaccination},
	Year = {2005},
	Bdsk-File-1 = {}}

