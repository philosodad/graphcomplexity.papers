	Journal = {Selected Areas in Communications, IEEE Journal on},
	Keywords = {access protocols, channel allocation, distributed algorithms, radio networks, routing protocols, telecommunication computing, telecommunication traffic, wireless channelsCCM, JCAR, Layer 2.5 JCAR, M3WN, MAC layer, channel cost metric, distributed algorithm, diverse channel characteristic, heterogeneous multiradio multichannel multihop wireless network, interfering channel, joint channel assignment-routing protocol, software solution, traffic information measurement, wireless interface},
	Month = {Nov.},
